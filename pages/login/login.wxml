<view class="logo" style="height:{{0.35*h}}px">
  <view class="title"><text>农产品质量安全追溯平台</text></view>
</view>
<form bindsubmit="formBindSubmit">
  <view class="section_message">
    <input name="user" placeholder="账号" value="{{user}}"/>
    <input name="password" password placeholder="密码" value="{{password}}"/>
  </view>
  <button form-type="submit" bindtap="listenerInput" class="login">登录</button>
  
</form>
